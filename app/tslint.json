{
	"rules": {
		"class-name": true,
		"no-duplicate-variable": true,
		"no-trailing-whitespace": true,
		"no-eval": true,
		"no-var-keyword": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": [
			true,
			"allow-null-check"
		],
		"indent": [
			true,
			2
		],
		"quotemark": [
			true,
			"single"
		],
		"typedef-whitespace": [
			true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			}
		],
		"typedef": [
			true,
			"call-signature",
			"parameter",
			"property-declaration",
			"variable-declaration",
			"member-variable-declaration"
		],
		"variable-name": [
			true,
			"ban-keywords"
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type"
		]
	},
	"rulesDirectory": [
		"node_modules/tslint-eslint-rules/dist/rules"
	]
}
